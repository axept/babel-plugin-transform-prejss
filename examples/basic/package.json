    "jss-from-postcss",
    "scss",
    "sass",
    "less",
    "stylus",
    "sugarss"
  ],
  "author": "Axept <office@axept.co>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/axept/jss-from-postcss/issues"
